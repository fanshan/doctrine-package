      "name": "Gauthier 'Rusty' Delamarre",
      "email": "gauthier@objective-php.org",
      "role": "lead"
    }
  ],
  "require": {
    "php": ">=7.0",
    "objective-php/application": "^2.0",
    "objective-php/services-factory": "^2.0",
    "objective-php/config": "^2.0",
    "doctrine/orm": ">=2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
